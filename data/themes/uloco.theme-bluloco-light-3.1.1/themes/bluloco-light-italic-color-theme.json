        "punctuation.operation.graphql",
        "punctuation.colon.graphql"
      ],
      "settings": {
        "foreground": "#7a82da"
      }
    },
    {
      "name": "Delimiters",
      "scope": "none",
      "settings": {
        "foreground": "#383a42"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#7a82da"
      }
    },
    {
      "name": "Keywords",
      "scope": [
